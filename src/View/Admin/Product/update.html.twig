{% extends 'Admin/layoutAdmin.html.twig' %}

{% block content %}
    <h1>Modifier le produit : {{ product.name }} </h1>


    {% if errors is not empty %}
        <div class="alert alert-danger">
            <ul>
                {% for error in errors %}
                    <li>{{ error }}</li>
                {% endfor %}
            </ul>
        </div>
    {% endif %}
    {% include 'Admin/Product/update_form.html.twig' %}
{% endblock %}